{% extends 'base.html.twig' %}

{% block body %}
    <h1>Asistencium</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ asistencium.id }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ asistencium.descripcion }}</td>
            </tr>
            <tr>
                <th>Presentismo</th>
                <td>{% if asistencium.presentismo %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Fechaalta</th>
                <td>{% if asistencium.fechaAlta %}{{ asistencium.fechaAlta|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Fechamodificacion</th>
                <td>{% if asistencium.fechaModificacion %}{{ asistencium.fechaModificacion|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Fechabaja</th>
                <td>{% if asistencium.fechaBaja %}{{ asistencium.fechaBaja|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('asistencia_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('asistencia_edit', { 'id': asistencium.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
