<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>AdminLTE 2 | General Form Elements</title>
  <!-- Tell the browser to be responsive to screen width -->
  <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
  <!-- Bootstrap 3.3.6 -->
  <link rel="stylesheet" href="{{asset('bundles/application/bootstrap/css/bootstrap.min.css')}}">
  <!-- Font Awesome -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
  <!-- Ionicons -->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
  <!-- Theme style -->
  <link rel="stylesheet" href="{{asset('bundles/application/dist/css/AdminLTE.min.css')}}">
  <!-- AdminLTE Skins. Choose a skin from css/skins
       folder instead of downloading all of them to reduce the load. -->
  <link rel="stylesheet" href="{{asset('bundles/application/dist/css/skins/_all-skins.min.css')}}">

  <link rel="stylesheet" href="{{asset('bundles/teachingclass/plugins/datatables/dataTables.bootstrap.css')}}">
  {% block stylesheets %}{% endblock %}


  <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
  <!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
  <![endif]-->
</head>
<body class="hold-transition skin-blue layout-top-nav">
  <div class="wrapper">

    <header class="main-header">
      <nav class="navbar navbar-static-top">
        <div class="container">
          <div class="navbar-header">
            <a href="../../index2.html" class="navbar-brand"><b>SIGES</b></a>
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse">
              <i class="fa fa-bars"></i>
            </button>
          </div>

          <!-- Collect the nav links, forms, and other content for toggling -->
          <div class="collapse navbar-collapse pull-left" id="navbar-collapse">
            <ul class="nav navbar-nav">
              <li >{% if app.user.username is defined %}<a href="#"> <i class="fa fa-sign-out"></i> Cerrar Clase Didáctica </a>{% else %}<a href="{{path('acceder')}}"><i class="fa fa-sign-out"></i>  Salir de la clase</a>{% endif %}</li>

            </ul>

          </div>
          <!-- /.navbar-collapse -->
          <!-- Navbar Right Menu -->
          <div class="navbar-custom-menu">
            <ul class="nav navbar-nav">

              <!-- User Account Menu -->
              <li class="dropdown user user-menu">
                <!-- Menu Toggle Button -->
                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                  <!-- The user image in the navbar-->
                  <img src="{{asset('bundles/teachingclass/dist/img/user2-160x160.jpg')}} " class="user-image" alt="User Image">
                  <!-- hidden-xs hides the username on small devices so only the image appears. -->
                  {% if app.user.username is defined %}<span class="hidden-xs">{{app.user.username}}</span>{% else %}Acceso Anónimo{% endif %}
                </a>
                <ul class="dropdown-menu">
                  {% if app.user.username is defined %}
                  <!-- The user image in the menu -->
                  <li class="user-header">
                    <img src="../../dist/img/user2-160x160.jpg" class="img-circle" alt="User Image">

                    <p>
                      Alexander Pierce - Web Developer
                      <small>Member since Nov. 2012</small>
                    </p>
                  </li>

                  <!-- Menu Footer-->
                  <li class="user-footer">
                    <div class="pull-right">
                      <a href="{{path('logout')}}" class="btn btn-default btn-flat">Sign out</a>
                    </div>
                  </li>
                </ul>
                {% endif %}
              </li>
            </ul>
          </div>
          <!-- /.navbar-custom-menu -->
        </div>
        <!-- /.container-fluid -->
      </nav>
    </header>
    <!-- Full Width Column -->
    <div class="content-wrapper">
      <div class="container-fluid">
        <!-- Content Header (Page header) -->
        <section class="content-header">
          <h1>
            {% block pageTitle %}{% endblock %}
            <small>{% block smallPageTitle %}{% endblock %}</small>
          </h1>
        </section>

        <!-- Main content -->
        <section class="content">
          {% block buttonsTop %}{% endblock %}

          {% block content %}{% endblock %}
        </section>
        <!-- /.content -->
      </div>
      <!-- /.container -->
    </div>
    <!-- /.content-wrapper -->
    <footer class="main-footer">
      <div class="container">
        <div class="pull-right hidden-xs">
          <b>Version</b> 1.0
        </div>
        Sistema desarrollado por estudiantes de la <strong>Universidad Nacional Arturo Jauretche</strong> - Proyecto de Software <strong>2017</strong>
      </div>
      <!-- /.container -->
    </footer>
  </div>
<!-- ./wrapper -->

<!-- jQuery 2.2.3 -->
<script src="{{asset('bundles/application/plugins/jQuery/jquery-2.2.3.min.js')}}"></script>
<!-- Bootstrap 3.3.6 -->
<script src="{{asset('bundles/application/bootstrap/js/bootstrap.min.js')}}"></script>

<script src="{{asset('bundles/application/plugins/datatables/jquery.dataTables.min.js')}}"></script>

<script src="{{asset('bundles/application/plugins/datatables/dataTables.bootstrap.min.js')}}"></script>
<!-- FastClick -->
<script src="{{asset('bundles/application/plugins/fastclick/fastclick.js')}}"></script>
<!-- AdminLTE App -->
<script src="{{asset('bundles/application/dist/js/app.min.js')}}"></script>
<!-- AdminLTE for demo purposes -->
<script src="{{asset('bundles/application/dist/js/demo.js')}}"></script>


<script type="text/javascript">
$(function () {
        $("#tablaCompleta").DataTable({
          "order": [[ 0, "desc" ]]
        });
        $('#tablaIncompleta').DataTable({
          "paging": true,
          "lengthChange": false,
          "searching": false,
          "ordering": false,
          "info": true,
          "autoWidth": false
        });
      });
      </script>
</script>

<script src="{{asset('bundles/application/plugins/flot/jquery.flot.min.js')}}"></script>
<!-- FLOT RESIZE PLUGIN - allows the chart to redraw when the window is resized -->
<script src="{{asset('bundles/application/plugins/flot/jquery.flot.resize.min.js')}}"></script>
<!-- FLOT PIE PLUGIN - also used to draw donut charts -->
<script src="{{asset('bundles/application/plugins/flot/jquery.flot.pie.min.js')}}"></script>
<!-- FLOT CATEGORIES PLUGIN - Used to draw bar charts -->
<script src="{{asset('bundles/application/plugins/flot/jquery.flot.categories.min.js')}}"></script>
{% block javascripts %}{% endblock %}

</body>
</html>
